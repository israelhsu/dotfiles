alias cls='clear'
alias dotfiles='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'
alias h='history 10'
alias hh='history 20'
alias hhh='history 40'
alias hhhh='history'
alias hless='history | less +G'
alias install-core='ninja -j 400 install-core'
alias install-devcore='ninja -j 400 install-devcore'
alias install-dist-test='ninja -j 400 install-dist-test'
alias install-mongo='ninja -j 400 install-mongo'
alias j='jobs -l'
alias lint='echo pylinters...; buildscripts/pylinters.py lint-all; echo eslint...; buildscripts/eslint.py fix; echo clang_format...; buildscripts/clang_format.py format-my'
alias ll='ls -l'
alias la='ls -a'
alias lf='ls -F'
alias lesN='less -N'
alias nv='nvim'
alias pd='pushd'
alias pd+='pushd +1'
alias pd++='pushd +2'
alias pd+++='pushd +3'
alias pd-='pushd -1'
alias pd--='pushd -2'
alias pd---='pushd -3'
alias scons-enter='./buildscripts/scons.py --variables-files=etc/scons/mongodbtoolchain_stable_clang.vars --link-model=dynamic --modules=enterprise --ninja generate-ninja ICECC=icecc CCACHE=ccache'
alias up='cd ..'
alias upp='cd ../..'
alias uppp='cd ../../..'
alias upppp='cd ../../../..'
alias uppppp='cd ../../../../..'

alias make_compile_commands="python3 \
    buildscripts/scons.py \
    CC=/opt/mongodbtoolchain/v4/bin/clang \
    CXX=/opt/mongodbtoolchain/v4/bin/clang++ \
    --dbg=on \
    --disable-warnings-as-errors \
    --modules= \
    generated-sources \
    compiledb"

